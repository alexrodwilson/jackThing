class Main {
   function void main(){
      var int nOfTrials, i;
      var LFSR state, tempState;
      let nOfTrials = 10000;
      let i = 0;
      let state = LFSR.decToBin(13, 16);
      
      while (i < nOfTrials) {
         let tempState = state;
         let state = LFSR.nextGalois(state);
         do tempState.dispose();
         let i = i + 1;
      }
      do state.dispose();
      do Output.moveCursor(5, 20);
      do Output.printInt(nOfTrials);
      return;
   }
}